# Fresh Trade Frontend Environment Variables
# Copy this file to .env and update the values

# API Configuration
VITE_API_URL=http://localhost:8001

# Optional: If you want to use different endpoints for different environments
# VITE_API_URL=https://your-production-api.com
# VITE_API_URL=https://your-staging-api.com

# App Configuration
VITE_APP_NAME=Fresh Trade
VITE_APP_VERSION=1.0.0

# Feature Flags (optional)
VITE_ENABLE_FORUM=true
VITE_ENABLE_MESSAGING=true
VITE_ENABLE_FAVORITES=true

# Upload Configuration (optional)
VITE_MAX_IMAGE_SIZE=5242880
VITE_MAX_IMAGES_PER_LISTING=3

# Debug Mode (optional)
VITE_DEBUG_MODE=false

VITE_CLOUDINARY_CLOUD_NAME=dr2knxtuq
VITE_CLOUDINARY_LISTINGS_PRESET=tamatas_images
VITE_CLOUDINARY_PROFILES_PRESET=tamatas_profiles